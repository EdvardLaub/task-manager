[program:{{ app_name }}]
command={{ venv_dir }}/bin/gunicorn --bind 127.0.0.1:{{ app_port }} --workers 3 app:app
directory={{ app_dir }}
user={{ app_user }}
autostart=true
autorestart=true
redirect_stderr=true
stdout_logfile=/var/log/supervisor/{{ app_name }}.log
environment=PATH="{{ venv_dir }}/bin"
